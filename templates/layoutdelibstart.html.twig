                        <img src="{{ asset('build/img/default-avatar.png') }}" alt="photoprofile"/>

                    {% endif %}

                    {% if app.user.picture!="" %}
                        <img src="/SIGES/public/images/users/{{ app.user.picture }}" alt="photoprofile"/>

                    {% endif %}


                </div>
                <div class="info">
